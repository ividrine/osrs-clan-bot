{
  "name": "osrs-clan-bot",
  "version": "1.0.0",
  "description": "discord bot that displays in game clan chat notifications from the runelite plugin project",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "dev:debug": "ts-node-dev --transpile-only --respawn --inspect=4321 --project tsconfig.json src/index.ts",
    "dev": "ts-node-dev --transpile-only --respawn --project tsconfig.dev.json src/server.ts",
    "watch": "ts-node-dev --poll --exit-child --respawn --transpile-only --ignore-watch node_modules src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Isaac Vidrine",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.1.2",
    "rimraf": "^5.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "discord.js": "^14.11.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.40.0",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1"
  }
}
